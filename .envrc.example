source_env ../..

# Concourse connection info
export CONCOURSE_URI=
export CONCOURSE_TARGET=
export CONCOURSE_USER=
export CONCOURSE_PASSWORD=

# Vault info
export VAULT_ADDR=
export VAULT_TOKEN=

# CF name
export PRODUCT_NAME=rabbitmq
export FOUNDATION_NAME=

# Do not change this variables
export PATH=$PATH:$PWD/bin

# Config repo git info
export PIPELINE_REPO=https://github.com/Altoros/concourse-rabbitmq.git
export PIPELINE_REPO_PRIVATE_KEY_PATH=...
